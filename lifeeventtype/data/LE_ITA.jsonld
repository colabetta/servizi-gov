    {
      "@id": "http://dati.gov.it/onto/controlledvocabulary/LE/6",
      "@type": "skos:Concept",
      "clvapit:hasRankOrder": "1",
      "dcterms:identifier": "6",
      "skos:definition": [
        {
          "@language": "en",
          "@value": "This life event groups public services related to the registration or the ownership of a vehicle such as a car, a motorcycle, etc."
        },
        {
          "@language": "it",
          "@value": "Questo evento della vita raggruppo servizi pubblici relativi alla registrazione e al possesso di un veicolo come una macchina, una moto, ecc."
        }
      ],
      "skos:inScheme": {
        "@id": "http://dati.gov.it/onto/controlledvocabulary/LE"
      },
      "skos:notation": "6",
      "skos:prefLabel": [
        {
          "@language": "it",
          "@value": "Registrazione/possesso veicolo"
        },
        {
          "@language": "en",
          "@value": "Registering/owning a vehicle"
        }
      ]
    },
    {
      "@id": "http://dati.gov.it/onto/controlledvocabulary/LE/7",
      "@type": "skos:Concept",
      "clvapit:hasRankOrder": "1",
      "dcterms:identifier": "7",
      "skos:definition": [
        {
          "@language": "en",
          "@value": "This life event groups public services related to the access to the public transportation such as for instance the renewal of a subscription, the purchase of a single bus ticket, etc."
        },
        {
          "@language": "it",
          "@value": "Questo evento della vita raggruppa servizi pubblici relativi all'accesso al trasporto pubblico come per esempio il rinnovo di un abbonamento, l'acquisto di un biglietto singolo per l'autobus, ecc. "
        }
      ],
      "skos:inScheme": {
        "@id": "http://dati.gov.it/onto/controlledvocabulary/LE"
      },
      "skos:notation": "7",
      "skos:prefLabel": [
        {
          "@language": "en",
          "@value": "Access to public transportation"
        },
        {
          "@language": "it",
          "@value": "Accesso al trasporto pubblico"
        }
      ]
    },
    {
      "@id": "http://dati.gov.it/onto/controlledvocabulary/LE/4",
      "@type": "skos:Concept",
      "clvapit:hasRankOrder": "1",
      "dcterms:identifier": "4",
      "skos:definition": [
        {
          "@language": "en",
          "@value": "This life event groups public services related to when someone retires from his job or becomes a senior."
        },
        {
          "@language": "it",
          "@value": "Questo evento della vita raggruppa servizi pubblici relativi al pensionamento di una persona"
        }
      ],
      "skos:inScheme": {
        "@id": "http://dati.gov.it/onto/controlledvocabulary/LE"
      },
      "skos:notation": "4",
      "skos:prefLabel": [
        {
          "@language": "it",
          "@value": "Pensionamento"
        },
        {
          "@language": "en",
          "@value": "Retirement"
        }
      ]
    },
    {
      "@id": "http://dati.gov.it/onto/controlledvocabulary/LE/5",
      "@type": "skos:Concept",
      "clvapit:hasRankOrder": "1",
      "dcterms:identifier": "5",
      "skos:definition": [
        {
          "@language": "en",
          "@value": "This life event groups public services related to driving a vehicle, for instance car, motorcycle."
        },
        {
          "@language": "it",
          "@value": "Questo evento della vita raggruppa servizi pubblici relativi alla guida di un veicolo per esempio una macchina, una moto"
        }
      ],
      "skos:inScheme": {
        "@id": "http://dati.gov.it/onto/controlledvocabulary/LE"
      },
      "skos:notation": "5",
      "skos:prefLabel": [
        {
          "@language": "it",
          "@value": "Richiesta o rinnovo patente"
        },
        {
          "@language": "en",
          "@value": "Applying to a driver's licence (or renewing an existing one)"
        }
      ]
    },
    {
      "@id": "http://dati.gov.it/onto/controlledvocabulary/LE/2",
      "@type": "skos:Concept",
      "clvapit:hasRankOrder": "1",
      "dcterms:identifier": "2",
      "skos:definition": [
        {
          "@language": "en",
          "@value": "This life event groups public services related to a severe health problem, for instance getting disabled."
        },
        {
          "@language": "it",
          "@value": "Questo evento della vita raggruppa servizi pubblici relativi a seri problemi di salute come per esempio diventare disabile"
        }
      ],
      "skos:inScheme": {
        "@id": "http://dati.gov.it/onto/controlledvocabulary/LE"
      },
      "skos:notation": "2",
      "skos:prefLabel": [
        {
          "@language": "en",
          "@value": "Disability"
        },
        {
          "@language": "it",
          "@value": "Invalidità"
        }
      ]
    },
    {
      "@id": "http://dati.gov.it/onto/controlledvocabulary/LE/3",
      "@type": "skos:Concept",
      "clvapit:hasRankOrder": "1",
      "dcterms:identifier": "3",
      "skos:definition": [
        {
          "@language": "en",
          "@value": "This life event groups public services for when someone looks for a new job, starts a new job, leaves or quits a particular job on his own, or when someone loses his job "
        },
        {
          "@language": "it",
          "@value": "Questo evento della vita raggruppa servizi pubblici utili a quelli che cercano un nuovo lavoro, iniziano un nuovo lavoro, lasciano un lavoro spontaneamente o perdono il lavoro"
        }
      ],
      "skos:inScheme": {
        "@id": "http://dati.gov.it/onto/controlledvocabulary/LE"
      },
      "skos:notation": "3",
      "skos:prefLabel": [
        {
          "@language": "it",
          "@value": "Ricerca di lavoro, avvio nuovo lavoro, disoccupazione"
        },
        {
          "@language": "en",
          "@value": "Looking for a job, starting a new job, becoming unemployed"
        }
      ]
    },
    {
      "@id": "http://dati.gov.it/onto/controlledvocabulary/LE/1",
      "@type": "skos:Concept",
      "clvapit:hasRankOrder": "1",
      "dcterms:identifier": "1",
      "skos:definition": [
        {
          "@language": "en",
          "@value": "This life event groups any public service related to education, for example pre-school education, elementary school, higher education and university, a study grant, ..."
        },
        {
          "@language": "it",
          "@value": "Questo evento della vita raggruppa qualsiasi servizio pubblico relativo all'educazione, per esempio l'educazione pre-scolastica, la scuola elementare, la scuola media e la scuola superiore, l'università, la borsa di studio, ..."
        }
      ],
      "skos:inScheme": {
        "@id": "http://dati.gov.it/onto/controlledvocabulary/LE"
      },
      "skos:notation": "1",
      "skos:prefLabel": [
        {
          "@language": "en",
          "@value": "Enrolling in school/university and/or applying for a study grant"
        },
        {
          "@language": "it",
          "@value": "Iscrizione Scuola/Università e/o richiesta borsa di studio"
        }
      ]
    },
    {
      "@id": "http://dati.gov.it/onto/controlledvocabulary/LE/14",
      "@type": "skos:Concept",
      "clvapit:hasRankOrder": "1",
      "dcterms:identifier": "14",
      "skos:definition": [
        {
          "@language": "en",
          "@value": "This life event groups public services that relate to when a relative passes a way, and cover the public services directly related to the decease of that person (for instance notifying the authorities, arranging the funeral…), as well as related to settling inheritance and donations."
        },
        {
          "@language": "it",
          "@value": "Questo evento della vita raggruppa servizi pubblici necessari quando una persona muore (per esempio notifica alle autorità, gestione del funerale, ecc.) e quelli relativi all'eredità e alle donazioni"
        }
      ],
      "skos:inScheme": {
        "@id": "http://dati.gov.it/onto/controlledvocabulary/LE"
      },
      "skos:notation": "14",
      "skos:prefLabel": [
        {
          "@language": "it",
          "@value": "Morte ed eredità"
        },
        {
          "@language": "en",
          "@value": "Death of a close relative and/or starting an inheritance procedure"
        }
      ]
    },
    {
      "@id": "http://dati.gov.it/onto/controlledvocabulary/LE/8",
      "@type": "skos:Concept",
      "clvapit:hasRankOrder": "1",
      "dcterms:identifier": "8",
      "skos:definition": [
        {
          "@language": "en",
          "@value": "This life event groups public services related to a person’s place of living, for instance, buying a house or a piece of land, building, renting a house or apartment…"
        },
        {
          "@language": "it",
          "@value": "Questo evento della vita raggruppa servizi pubblici relativi a un posto dove una persona vive: Esempi di servizi pubblici includono: comprare una casa o una parte di terreno, costruire o ristrutturare un appartamento/casa, ecc."
        }
      ],
      "skos:inScheme": {
        "@id": "http://dati.gov.it/onto/controlledvocabulary/LE"
      },
      "skos:notation": "8",
      "skos:prefLabel": [
        {
          "@language": "it",
          "@value": "Compravendita/affitto casa/edifici/terreni, costruzione o ristrutturazione casa/edificio"
        },
        {
          "@language": "en",
          "@value": "Buying/renting a house/building/land, building, renovating a house/building"
        }
      ]
    },
    {
      "@id": "http://dati.gov.it/onto/controlledvocabulary/LE/9",
      "@type": "skos:Concept",
      "clvapit:hasRankOrder": "1",
      "dcterms:identifier": "9",
      "skos:definition": [
        {
          "@language": "it",
          "@value": "Questo evento della vita raggruppa servizi pubblici relativi al cambio di residenza o di domicilio sia per cittadini italiani sia per cittadini stranieri presenti nel territorio"
        },
        {
          "@language": "en",
          "@value": "This life event groups public services related to moving or changing address both for Italian citizens and foreigns who are in the country"
        }
      ],
      "skos:inScheme": {
        "@id": "http://dati.gov.it/onto/controlledvocabulary/LE"
      },
      "skos:notation": "9",
      "skos:prefLabel": [
        {
          "@language": "en",
          "@value": "Moving and changing address within one country"
        },
        {
          "@language": "it",
          "@value": "Cambio di residenza/domicilio"
        }
      ]
    },
    {
      "@id": "dcterms:identifier",
      "@type": "rdfs:Property"
    },
    {
      "@id": "skos:notation",
      "@type": "rdfs:Property"
    },
    {
      "@id": "http://dati.gov.it/onto/controlledvocabulary/LE/16",
      "@type": "skos:Concept",
      "clvapit:hasRankOrder": "1",
      "dcterms:identifier": "16",
      "skos:definition": [
        {
          "@language": "it",
          "@value": "Questo evento della vita raggruppa servizi pubblici relativi a un crimine, per esempio nel caso in cui una persona è vittima o testimone di uno specifico crimine, ecc."
        },
        {
          "@language": "en",
          "@value": "This life event groups public services related to a crime, for instance in case you are the committer of that crime, or the victim or witness of a particular crime."
        }
      ],
      "skos:inScheme": {
        "@id": "http://dati.gov.it/onto/controlledvocabulary/LE"
      },
      "skos:notation": "16",
      "skos:prefLabel": [
        {
          "@language": "it",
          "@value": "Denuncia crimini"
        },
        {
          "@language": "en",
          "@value": "Reporting a crime (smaller offences, e.g., theft, burglary, etc)"
        }
      ]
    },
    {
      "@id": "http://dati.gov.it/onto/controlledvocabulary/LE/17",
      "@type": "skos:Concept",
      "clvapit:hasRankOrder": "1",
      "dcterms:identifier": "17",
      "skos:inScheme": {
        "@id": "http://dati.gov.it/onto/controlledvocabulary/LE"
      },
      "skos:notation": "17",
      "skos:prefLabel": [
        {
          "@language": "it",
          "@value": "Dichiarazione dei redditi, versamento e riscossione tributi/imposte e contributi"
        },
        {
          "@language": "en",
          "@value": "Declaring income taxes, paying contributions"
        }
      ]
    },
    {
      "@id": "dcterms:type",
      "@type": "rdfs:Property"
    },
    {
      "@id": "http://dati.gov.it/onto/controlledvocabulary/LE/15",
      "@type": "skos:Concept",
      "clvapit:hasRankOrder": "1",
      "dcterms:identifier": "15",
      "skos:inScheme": {
        "@id": "http://dati.gov.it/onto/controlledvocabulary/LE"
      },
      "skos:notation": "15",
      "skos:prefLabel": [
        {
          "@language": "en",
          "@value": "Making/cancelling a doctor's appointment"
        },
        {
          "@language": "it",
          "@value": "Prenotazione e disdetta visite/esami"
        }
      ]
    },
    {
      "@id": "http://dati.gov.it/onto/controlledvocabulary/LE/12",
      "@type": "skos:Concept",
      "clvapit:hasRankOrder": "1",
      "dcterms:identifier": "12",
      "skos:definition": [
        {
          "@language": "en",
          "@value": "This life event groups public services related to becoming a caretaker for a child, for instance in case of giving birth, adopting, receiving a foster child…"
        },
        {
          "@language": "it",
          "@value": "Questo evento della vita raggruppa servizi pubblici relativi alla nascita di un bambino, all'adozione di un bambino, ecc."
        }
      ],
      "skos:inScheme": {
        "@id": "http://dati.gov.it/onto/controlledvocabulary/LE"
      },
      "skos:notation": "12",
      "skos:prefLabel": [
        {
          "@language": "en",
          "@value": "Declaring the birth of a child and/or applying for a birth grand"
        },
        {
          "@language": "it",
          "@value": "Nascita di un bambino, richiesta adozioni"
        }
      ]
    },
    {
      "@id": "http://dati.gov.it/onto/controlledvocabulary/LE/13",
      "@type": "skos:Concept",
      "clvapit:hasRankOrder": "1",
      "dcterms:identifier": "13",
      "skos:definition": [
        {
          "@language": "en",
          "@value": "This life event groups public services related to a person’s official relationship, for instance marriage, registered partnership, divorce…"
        },
        {
          "@language": "it",
          "@value": "Questo evento della vita raggruppa servizi pubblici relativi a una relazione ufficiale di una persona, come per esempio il matrimonio, l'unione civile, il divorzio,..."
        }
      ],
      "skos:inScheme": {
        "@id": "http://dati.gov.it/onto/controlledvocabulary/LE"
      },
      "skos:notation": "13",
      "skos:prefLabel": [
        {
          "@language": "it",
          "@value": "Matrimonio e/o cambio stato civile"
        },
        {
          "@language": "en",
          "@value": "Marrying and/or changing martal status"
        }
      ]
    },
    {
      "@id": "http://dati.gov.it/onto/controlledvocabulary/LE/10",
      "@type": "skos:Concept",
      "clvapit:hasRankOrder": "1",
      "dcterms:identifier": "10",
      "skos:definition": [
        {
          "@language": "it",
          "@value": "Questo evento della vita raggruppa servizi pubblici relativi a quando qualcuno espatria (per diversi motivi che possono riguardare: il lavoro, lo studio, il pensionamento)"
        },
        {
          "@language": "en",
          "@value": "This life event groups public services that relate to when someone moves from one country to another."
        }
      ],
      "skos:inScheme": {
        "@id": "http://dati.gov.it/onto/controlledvocabulary/LE"
      },
      "skos:notation": "10",
      "skos:prefLabel": [
        {
          "@language": "en",
          "@value": "Moving or preparing to move to another country (e.g., to study, work, retire, ..)"
        },
        {
          "@language": "it",
          "@value": "Espatrio per lavoro, studio, pensionamento"
        }
      ]
    },
    {
      "@id": "http://dati.gov.it/onto/controlledvocabulary/LE/11",
      "@type": "skos:Concept",
      "clvapit:hasRankOrder": "1",
      "dcterms:identifier": "11",
      "skos:definition": [
        {
          "@language": "it",
          "@value": "Questo evento della vita raggruppa servizi pubblici relativi alle necessità di una persona nel caso di viaggi all'estero. Per esempio: ottenere un passaporto, un visto, una vaccinazione, ecc."
        },
        {
          "@language": "en",
          "@value": "This life events groups public services related to what you need to do when travelling abroad, for instance getting an international passport or driving license, getting a visa, getting vaccination…"
        }
      ],
      "skos:inScheme": {
        "@id": "http://dati.gov.it/onto/controlledvocabulary/LE"
      },
      "skos:notation": "11",
      "skos:prefLabel": [
        {
          "@language": "it",
          "@value": "Richiesta passaporto, visto e assistenza viaggi internazionali"
        },
        {
          "@language": "en",
          "@value": "Needing a passport/visa or assistance to travel to another country"
        }
      ]
    },
    {
      "@id": "skos:definition",
      "@type": "rdfs:Property"
    },
    {
      "@id": "http://dati.gov.it/onto/controlledvocabulary/LE/18",
      "@type": "skos:Concept",
      "clvapit:hasRankOrder": "1",
      "dcterms:identifier": "18",
      "skos:definition": [
        {
          "@language": "it",
          "@value": "Questo evento della vita raggruppa servizi pubblici relativi all'accesso ai luoghi della cultura pubblici come per esempio il pagamento di un biglietto di entrata, o l'uso di una biblioteca pubblica"
        },
        {
          "@language": "en",
          "@value": "This life event groups public services related to the access to public cultural sites such as the payment of a ticket or the use of public library"
        }
      ],
      "skos:inScheme": {
        "@id": "http://dati.gov.it/onto/controlledvocabulary/LE"
      },
      "skos:notation": "18",
      "skos:prefLabel": [
        {
          "@language": "it",
          "@value": "Accesso luoghi della cultura"
        },
        {
          "@language": "en",
          "@value": "Access to public cultural sites"
        }
      ]
    },
    {
      "@id": "http://dati.gov.it/onto/controlledvocabulary/LE/19",
      "@type": "skos:Concept",
      "clvapit:hasRankOrder": "1",
      "dcterms:identifier": "19",
      "skos:definition": [
        {
          "@language": "en",
          "@value": "This life groups public services related to the people who owns a pet. Examples include: communicate the transfer of a dog to another, the death of a dog, etc."
        },
        {
          "@language": "it",
          "@value": "Questo evento della vita raggruppa servizi pubbici che possono essere utilizzati dalle persone che hanno un animale da compagnia. Esempi includono: comunicazione di cessione di un cane ad un altro, decesso di un cane, ecc."
        }
      ],
      "skos:inScheme": {
        "@id": "http://dati.gov.it/onto/controlledvocabulary/LE"
      },
      "skos:notation": "19",
      "skos:prefLabel": [
        {
          "@language": "it",
          "@value": "Possesso, cura, smarrimento animale da compagnia"
        },
        {
          "@language": "en",
          "@value": "Owning, taking care, losing pets"
        }
      ]
    }
  ]
}
