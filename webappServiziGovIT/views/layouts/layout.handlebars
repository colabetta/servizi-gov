                    </a>
                    <div class="dropdown-menu dropdown-menu-right">
                        <div class="dropdown-header text-center">
                            <strong>Impostazioni</strong>
                        </div>
                        <a class="dropdown-item" href="../profilo">
                            <i class="fa fa-user"></i> Profilo</a>
                        {{#if (checkIsAdmin user)}}
                        <a class="dropdown-item" href="/accesscontrol/index">
                            <i class="fa fa-users"></i> Gestione Utenti </a>
                        {{/if}}
                        <a class="dropdown-item" href="/users/logout">
                            <i class="fa fa-lock"></i> Logout </a>
                    </div>
                </li>
                {{!-- <li class="nav-item d-md-down-none">
                    <a class="nav-link" href="#">
                        <i class="icon-bell"></i>
                        <span class="badge badge-pill badge-danger">5</span>
                    </a>
                </li> --}}
                {{else}}
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle nav-link" data-toggle="dropdown" href="/users/login" role="button" aria-haspopup="true"
                        aria-expanded="false">
                        <span class="d-md-down-none">Accedi</span>
                    </a>
                </li>
                <li class="nav-item d-md-down-none">
                    {{!-- <a class="nav-link navbar-toggler aside-menu-toggler" href="#">?</a> --}}
                </li>
                {{/if}}
            </ul>
            <input type="hidden" id="userId" value="" /> {{!--
            <input type="hidden" id="codiceIpa" value="{{client.set(" userId ", user.codicefiscale, function(err, reply) {
        console.log(reply);
            });}}" /> --}}
            <input type="hidden" id="templateRef" value="" />
            <input type="hidden" id="serviceId" value="" />
            <input type="hidden" id="codiceIpa" value="" />


        </header>

        {{{body}}}

        <footer class="app-footer">
            <a href="http://www.agid.gov.it">
