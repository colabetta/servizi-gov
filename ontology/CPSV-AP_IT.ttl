                 rdfs:label "alternative name"@en ,
                            "nome alternativo"@it ;
                 
                 rdfs:comment "This property is used to represent an alternative name for a legal entity."@en ,
                              "Questa proprietà è utilizzata per rappresentare un nome alternativo per una entità legale."@it ;
                 
                 rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
                 
                 rdfs:domain :LegalEntity ;
                 
                 rdfs:range rdfs:Literal ;
                 
                 rdfs:subPropertyOf <http://xmlns.com/foaf/0.1/name> .



###  http://dati.gov.it/onto/cpsvapit#endInterval

:endInterval rdf:type owl:DatatypeProperty ;
             
             rdfs:label "end interval"@en ,
                        "fine intervallo"@it ;
             
             rdfs:comment "This property represents the end of a time interval and it can assume a value of type date, dateTime, time."@en ,
                          "Questa proprietà rappresenta la fine di un intervallo temporale e può assumere un valore di tipo date, dateTime o time."@it ;
             
