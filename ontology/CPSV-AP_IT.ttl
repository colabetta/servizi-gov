                                owl:onDataRange [ rdf:type rdfs:Datatype ;
                                                  owl:unionOf ( xsd:date
                                                                xsd:dateTime
                                                                xsd:time
                                                              )
                                                ]
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasDayWeek ;
                                owl:allValuesFrom :DayOfTheWeek
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :startInterval ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onDataRange [ rdf:type rdfs:Datatype ;
                                                  owl:unionOf ( xsd:date
                                                                xsd:dateTime
                                                                xsd:time
                                                              )
                                                ]
                              ] ;
              
              rdfs:comment "This class is used to represent a time interval."@en ,
                           "Questa classe è utilizzata per rappresentare un intervallo temporale."@it ;
              
              rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> .



###  http://dati.gov.it/onto/cpsvapit#WebSite

:WebSite rdf:type owl:Class ;
         
         rdfs:label "Web Site"@en ,
                    "Sito Web"@it ;
         
         rdfs:subClassOf :Channel ,
                         <http://xmlns.com/foaf/0.1/Document> ;
         
         rdfs:comment "This class represents a channel of type web site."@en ,
                      "Questa classe rappresenta un canale di tipo sito web."@it ;
         
         rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> .



###  http://dati.gov.it/onto/dcatapit#Agent

<http://dati.gov.it/onto/dcatapit#Agent> rdf:type owl:Class ;
                                         
                                         rdfs:label "Agent"@en ,
                                                    "Agente"@it ;
                                         
                                         rdfs:comment "This class has been defined in the DCAT-AP_IT Italian profile for metadata that describe data of PAs.This alignment guarantees the linking between CPSV-AP_IT and DCAT-AP_IT."@en ,
                                                      "Questa classe è definita in DCAT-AP_IT ovvero il profilo italiano DCAT-AP per i metadati che descrivono i dati delle PA. Questo allineamento garntisce un collegamento tra le due ontologie CPSV-AP_IT e DCAT-AP_IT."@it ,
                                                      <http://dati.gov.it/onto/dcatapit> .



###  http://dati.gov.it/onto/dcatapit#LicenseDocument

<http://dati.gov.it/onto/dcatapit#LicenseDocument> rdf:type owl:Class ;
                                                   
                                                   rdfs:label "License Document"@en ,
                                                              "Licenza"@it ;
                                                   
                                                   rdfs:comment "This class is defined in DCAT-AP_IT and it has been added in order to guarantee the alingment between CPSV-AP_IT and DCAT-AP_IT."@en ,
                                                                "Questa classe è definita in DCAT-AP_IT ed è stata aggiunta al fine di allineare CPSV-AP_IT a DCAT-AP_IT."@it ;
                                                   
                                                   rdfs:isDefinedBy <http://dati.gov.it/onto/dcatapit> .



###  http://dati.gov.it/onto/dcatapit#Organization

<http://dati.gov.it/onto/dcatapit#Organization> rdf:type owl:Class ;
                                                
                                                rdfs:label "Organization"@en ,
