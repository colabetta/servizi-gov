@prefix : <http://dati.gov.it/onto/cpsv-itap/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://dati.gov.it/onto/cpsvapit> .

<http://dati.gov.it/onto/cpsvapit> rdf:type owl:Ontology ;
                                   
                                   dc:issued "2015-11-21" ;
                                   
                                   dc:creator "Agency for Digital Italy - AgID"@en ;
                                   
                                   dc:title "Core Public Service Vocabulary - The Italian Application Profile (CPSV-AP_IT)"@en ;
                                   
                                   dc:description "This is the ontology of the Italian application profile for public services. With the term \"public service\" we mean any discretional or mandatory act carried out by a Public Administration (or on behalf of a Public Administration) for the advantage of citizens and enterprises."@en ;
                                   
                                   rdfs:comment "This is the ontology of the Italian application profile for public services. With the term \"public service\" we mean any discretional or mandatory act carried out by a Public Administration (or on behalf of a Public Administration) for the advantage of citizens and enterprises."@en ;
                                   
                                   owl:versionInfo "Version 0.4 - 8 February 2016"@en ;
                                   
                                   dc:creator "Agenzia per l'Italia Digitale - AgID"@it ;
                                   
                                   dc:title "Core Public Service Vocabulary - Il profilo applicativo italiano (CPSV-AP_IT)"@it ;
                                   
                                   dc:description "Ontologia del profilo italiano per la descrizione dei servizi pubblici. Per servizio pubblico si intende qualsiasi atto obbligatorio o discrezionale espletato da una pubblica amministrazione (o per conto di una pubblica amministrazione) nei confronti di cittadini, imprese/professionisti."@it ;
                                   
                                   rdfs:comment "Ontologia del profilo italiano per la descrizione dei servizi pubblici. Per servizio pubblico si intende qualsiasi atto obbligatorio o discrezionale espletato da una pubblica amministrazione (o per conto di una pubblica amministrazione) nei confronti di cittadini, imprese/professionisti."@it ;
                                   
                                   owl:versionInfo "Versione 0.4 - 8 febbraio 2016"@it ;
                                   
                                   rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
                                   
                                   owl:versionIRI <http://dati.gov.it/onto/cpsvapit/0.4> .


#################################################################
#
#    Datatypes
#
#################################################################


###  http://www.w3.org/2001/XMLSchema#date

xsd:date rdf:type rdfs:Datatype .



###  http://www.w3.org/2001/XMLSchema#time

xsd:time rdf:type rdfs:Datatype .





#################################################################
#
#    Object Properties
#
#################################################################


###  http://dati.gov.it/onto/cpsvapit#actsOnBehalfOf

:actsOnBehalfOf rdf:type owl:ObjectProperty ;
                
                rdfs:label "acts on behalf of"@en ,
                           "agisce per conto di"@it ;
                
                rdfs:comment "This property represents an accountability relationship that exists between a person and a legal entity where the person works."@en ,
                             "Questa proprietà rappresenta una relazione, che intercorre tra una persona e un'entità legale, di responsabilità, nello specifico della persona nei confronti dell'entità legale (organizzazione privata o pubblica) presso la quale la persona si colloca."@it ;
                
                rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
                
                rdfs:range :Agent ;
                
                rdfs:domain :Agent ;
                
                rdfs:subPropertyOf <http://www.w3.org/ns/org#reportsTo> .



###  http://dati.gov.it/onto/cpsvapit#currency

:currency rdf:type owl:ObjectProperty ;
          
          rdfs:label "currency"@en ,
                     "valuta"@it ;
          
          rdfs:comment "This property is used to model the currency associated with the cost of the service. The currency must be chosen using the following controlled vocabulary: http://publications.europa.eu/resource/authority/currency"@en ,
                       "Questa proprietà è utilizzata per specificare la valuta da associare al costo del servizio. La valuta deve essere scelta in base al seguente vocabolario controllato: http://publications.europa.eu/resource/authority/currency"@it ;
          
          rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
          
          rdfs:domain :Cost ;
          
          rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> .



###  http://dati.gov.it/onto/cpsvapit#follows

:follows rdf:type owl:ObjectProperty ;
         
         rdfs:label "follows"@en ,
                    "rispetta"@it ;
         
         rdfs:comment "This property links a public service to the set of rules the service has to follow in order to be carried out."@en ,
                      "Questa proprietà è utilizzata per poter collegare il servizio pubblico all'insieme di regole che esso deve rispettare per il suo espletamento."@it ;
         
         rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
         
         rdfs:domain :PublicService ;
         
         rdfs:range :Rule ;
         
         owl:inverseOf :isFollowedBy .



###  http://dati.gov.it/onto/cpsvapit#hasChannel

:hasChannel rdf:type owl:ObjectProperty ;
            
            rdfs:label "has channel"@en ,
                       "ha canale"@it ;
            
            rdfs:comment "This property is used to link the public service with the channel through which it is provided to the end users. It is recommended specifying the channel through the service is provided."@en ,
                         "Questa classe collega il servizio pubblico al canale attraverso cui erogare il servizio pubblico agli utenti finali. E' raccomandato specificare qual è il canale di erogazione per un servizio pubblico."@it ;
            
            rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
            
            rdfs:range :Channel ;
            
            rdfs:domain :PublicService .



###  http://dati.gov.it/onto/cpsvapit#hasContactPoint

:hasContactPoint rdf:type owl:ObjectProperty ;
                 
                 rdfs:label "has contact point"@en ,
                            "ha punto di contatto"@it ;
                 
                 rdfs:comment "Questa proprietà è utilizzata per specificare un punto di contatto. Per esempio essa può collegare un servizio pubblico a uno o più punti di contatto."@en ,
                              "This property is used to represent the point of contact. For instance it can be used to link the public service with its contant point. More than one contact point for a service can be specified."@it ;
                 
                 rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
                 
                 owl:inverseOf :isContactPointOf ;
                 
                 rdfs:range <http://dati.gov.it/onto/dcatapit#Organization> ;
                 
                 rdfs:domain owl:Thing .



###  http://dati.gov.it/onto/cpsvapit#hasCost

:hasCost rdf:type owl:ObjectProperty ;
         
         rdfs:label "has cost"@en ,
                    "ha costo"@it ;
         
         rdfs:comment "This property links a public service to the costs associated with it."@en ,
                      "Questa proprietà consente di collegare un servizio pubblico ai costi associati ad esso."@it ;
         
         rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
         
         rdfs:range :Cost ;
         
         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( :BusinessEvent
                                     :PublicService
                                   )
                     ] .



###  http://dati.gov.it/onto/cpsvapit#hasDayWeek

:hasDayWeek rdf:type owl:ObjectProperty ;
            
            rdfs:label "has day of the week"@en ,
                       "ha giorno della settimana"@it ;
            
            rdfs:comment "Property used to link a time interval with a day of the week."@en ,
                         "Proprietà che collega un intervallo temporale al giorno della settimana."@it ;
            
            rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
            
            rdfs:range :DayOfTheWeek ;
            
            rdfs:domain :TimeInterval .



###  http://dati.gov.it/onto/cpsvapit#hasEmail

:hasEmail rdf:type owl:ObjectProperty ;
          
          rdfs:label "has email"@en ,
                     "ha email"@it ;
          
          rdfs:comment "This property is a sub property of has channel and it is used to connect a public service with a specific type of channel; i.e., email."@en ,
                       "Questa proprietà è una sotto proprietà di \"ha canale\" ed è utilizzata al fine di legare un servizio pubblico a uno specifico tipo di canale; i.e., email."@it ;
          
          rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
          
          rdfs:range :Email ;
          
          rdfs:domain :PublicService ;
          
          rdfs:subPropertyOf :hasChannel ,
                             <http://www.w3.org/2006/vcard/ns#hasEmail> .



###  http://dati.gov.it/onto/cpsvapit#hasInput

:hasInput rdf:type owl:ObjectProperty ;
          
          rdfs:label "has input"@en ,
                     "ha input"@it ;
          
          rdfs:comment "This property is used to link a public service with an input required to carry out the service. A service can have more than one input. It is mandatory to specify an input for a public service."@en ,
                       "Questa proprietà è utilizzata per collegare un servizio pubblico all'input richiesto per poter espletare il servizio stesso. Il servizio può avere più di un input. E' obbligatorio specificare un input per il servizio."@it ;
          
          rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
          
          rdfs:range :Input ;
          
          rdfs:domain :PublicService .



###  http://dati.gov.it/onto/cpsvapit#hasMeasurementUnit

:hasMeasurementUnit rdf:type owl:ObjectProperty ;
                    
                    rdfs:label "has measurement unit"@en ,
                               "ha unità di misura"@it ;
                    
                    rdfs:comment "This property models the measurement unit to be assigned to a quantitative value. It connects the class Quantitative Value with the class Mearurement Unit."@en ,
                                 "Questa proprietà consente di specificare un'unità di misura per un valore quantitativo. Essa connette la classe Quantitative Value alla classe Measurement Unit."@it ;
                    
                    rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
                    
                    rdfs:range :MeasurementUnit ;
                    
                    rdfs:domain :QuantitativeValue .



###  http://dati.gov.it/onto/cpsvapit#hasOtherElectronicCh

:hasOtherElectronicCh rdf:type owl:ObjectProperty ;
                      
                      rdfs:label "has other electronic channel"@en ,
                                 "ha altro canale elettronico"@it ;
                      
                      rdfs:comment "This property is subproperty of \"has channel\" and it is used to link a public service with another electronic channel."@en ,
                                   "Questa proprietà è sotto proprietà di \"ha canale\" ed è utilizzata per collegare un servizio pubblico a un canale di tipo non elettronico."@it ;
                      
                      rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
                      
                      rdfs:range :OtherElectronicChannel ;
                      
                      rdfs:domain :PublicService ;
                      
                      rdfs:subPropertyOf :hasChannel .



###  http://dati.gov.it/onto/cpsvapit#hasPhone

:hasPhone rdf:type owl:ObjectProperty ;
          
          rdfs:label "has telephone"@en ,
                     "ha telefono"@it ;
          
          rdfs:comment "This property is a subproperty of \"has channel\" and it is used to link the public service with a specific channel type; i.e., telephone."@en ,
                       "Questa proprietà è una sottoproprietà di \"ha canale\" ed è utilizzata per connettere un servizio pubblico a uno specifico tipo di canale; i.e., telefono."@it ;
          
          rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
          
          rdfs:range :Phone ;
          
          rdfs:domain :PublicService ;
          
          rdfs:subPropertyOf :hasChannel ,
                             <http://www.w3.org/2006/vcard/ns#hasTelephone> .



###  http://dati.gov.it/onto/cpsvapit#hasStatus

:hasStatus rdf:type owl:ObjectProperty ;
           
           rdfs:label "has status"@en ,
                      "ha stato"@it ;
           
           rdfs:comment "Property used to link a public service with a status."@en ,
                        "Proprietà utilizzata per collegare un servizio pubblico a uno stato."@it ,
                        <http://dati.gov.it/onto/cpsvapit> ;
           
           rdfs:domain :PublicService ;
           
           rdfs:range :Status ;
           
           owl:inverseOf :isStatusFor .



###  http://dati.gov.it/onto/cpsvapit#hasWebSite

:hasWebSite rdf:type owl:ObjectProperty ;
            
            rdfs:label "has web site"@en ,
                       "ha sito web"@it ;
            
            rdfs:comment "This property is a subproperty of \"has channel\" and it is used to link the public service with a specific type of channel; i.e., web site."@en ,
                         "Questa proprietà è sottoporprietà di \"ha canale\" ed è utilizzata per collegare un servizio pubblico a uno specifico tipo di canale; i.e., sito web."@it ;
            
            rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
            
            rdfs:domain :PublicService ;
            
            rdfs:range :WebSite ;
            
            rdfs:subPropertyOf :hasChannel ,
                               <http://xmlns.com/foaf/0.1/homepage> .



###  http://dati.gov.it/onto/cpsvapit#implements

:implements rdf:type owl:ObjectProperty ;
            
            rdfs:label "implements"@en ,
                       "implementa"@it ;
            
            rdfs:comment "This property is used to represent rules that implement specific legislation or policy documents."@en ,
                         "Questa proprietà è utilizzata per indicare regole che implementano spcifici riferimenti normativi."@it ;
            
            rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
            
            rdfs:range :FormalFramework ;
            
            rdfs:domain :Rule ;
            
            owl:inverseOf :isImplementedBy .



###  http://dati.gov.it/onto/cpsvapit#interactivityLevel

:interactivityLevel rdf:type owl:ObjectProperty ;
                    
                    rdfs:label "interactivity level"@en ,
                               "livello interattivo"@it ;
                    
                    rdfs:comment "this property is used to represent the interactivity level of the service. The interactivity level must be chosen through the use of the following controlled vocabulary:  http://dati.gov.it/onto/controlledvocabulary/InteractivityLevel"@en ,
                                 "Questa proprietà consente di rappresentare il livello di interattività del servizio. Il livello di interattività deve essere specificato mediante l'uso del vocabolario controllato:  http://dati.gov.it/onto/controlledvocabulary/InteractivityLevel"@it ;
                    
                    rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
                    
                    rdfs:domain :PublicService ;
                    
                    rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> .



###  http://dati.gov.it/onto/cpsvapit#isChannelOf

:isChannelOf rdf:type owl:ObjectProperty ;
             
             rdfs:label "is channel of"@en ,
                        "è canale di"@it ;
             
             rdfs:comment "Inverse property of \"has channel\" used to link a channel with the public service."@en ,
                          "Proprietà inversa di \"ha canale\" utilizzata per collegare un canale con un servizio pubblico."@it ;
             
             rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
             
             rdfs:domain :Channel ;
             
             rdfs:range :PublicService ;
             
             owl:inverseOf :hasChannel .



###  http://dati.gov.it/onto/cpsvapit#isContactPointOf

:isContactPointOf rdf:type owl:ObjectProperty ;
                  
                  rdfs:label "is contact point of"@en ,
                             "è punto di contatto per"@en ;
                  
                  rdfs:comment "This is the inverse property of \"has contact point\"."@en ,
                               "Questa è la proprietà inversa di \"ha punto di contatto\"."@it ;
                  
                  rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
                  
                  rdfs:domain <http://dati.gov.it/onto/dcatapit#Organization> ;
                  
                  rdfs:range owl:Thing .



###  http://dati.gov.it/onto/cpsvapit#isFollowedBy

:isFollowedBy rdf:type owl:ObjectProperty ;
              
              rdfs:label "is followed by"@en ,
                         "è rispettata da"@it ;
              
              rdfs:comment "This property is the inverse of \"follows\" and links the \"Rule\" with the \"Public Service\"."@en ,
                           "Questa è la proprietà inversa di \"rispetta\" che connette una regola al servizio pubblico."@it ;
              
              rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
              
              rdfs:range :PublicService ;
              
              rdfs:domain :Rule .



###  http://dati.gov.it/onto/cpsvapit#isImplementedBy

:isImplementedBy rdf:type owl:ObjectProperty ;
                 
                 rdfs:label "is implemented by"@en ,
                            "è implementato da"@it ;
                 
                 rdfs:comment "This property is the inverse of \"implements\" used to link the \"Formal Framework\" with the \"Rule\"."@en ,
                              "Questa è la proprietà inversa di \"implementa\" che connette il \"Framework Legale\" alla \"Regola\"."@it ;
                 
                 rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
                 
                 rdfs:domain :FormalFramework ;
                 
                 rdfs:range :Rule .



###  http://dati.gov.it/onto/cpsvapit#isPhysicalLocationOf

:isPhysicalLocationOf rdf:type owl:ObjectProperty ;
                      
                      rdfs:label "is the physical location of"@en ,
                                 "è la localizzazione fisica di"@it ;
                      
                      rdfs:comment "Inverse property of \"is physically available at\" that links a physical location to the public service."@en ,
                                   "Proprietà inversa di \"è disponibile fisicamente presso\" che lega una localizzazione fisica a il servizio pubblico."@it ;
                      
                      rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
                      
                      rdfs:domain :Location ;
                      
                      rdfs:range :PublicService ;
                      
                      owl:inverseOf :isPhysicallyAvailableAt .



###  http://dati.gov.it/onto/cpsvapit#isPhysicallyAvailableAt

:isPhysicallyAvailableAt rdf:type owl:ObjectProperty ;
                         
                         rdfs:label "is physically available at"@en ,
                                    "è fisicamente disponibile presso"@it ;
                         
                         rdfs:comment "This property links the public service to its physical location in the case the service is provided offline at some public administration's desk/totem."@en ,
                                      "Questa proprietà collega un servizio pubblico alla locazione fisica che lo eroga nel caso il servizio sia offerto agli utenti finali via sportelli fisici/totem."@it ,
                                      <http://dati.gov.it/onto/cpsvapit> ;
                         
                         rdfs:range :Location ;
                         
                         rdfs:domain :PublicService ;
                         
                         rdfs:subPropertyOf :hasChannel .



###  http://dati.gov.it/onto/cpsvapit#isPlayedBy

:isPlayedBy rdf:type owl:ObjectProperty ;
            
            rdfs:label "is played by"@en ,
                       "è esercitato da"@it ;
            
            rdfs:comment "Inverse property of \"plays a role in time\" used to link the situation \"role in time\" to the Agent."@en ,
                         "Proprietà inversa di \"esercita un ruolo nel tempo\" utilizzata per collegare la situazione ruolo nel tempo con il Soggetto."@it ,
                         <http://dati.gov.it/onto/cpsvapit> ;
            
            rdfs:range :Agent ;
            
            rdfs:domain :RoleInTime .



###  http://dati.gov.it/onto/cpsvapit#isRelatedToRiT

:isRelatedToRiT rdf:type owl:ObjectProperty ;
                
                rdfs:label "is related to role in time"@en ,
                           "è relativo al ruolo nel tempo"@it ;
                
                rdfs:comment "Inverse property of \"role for\"."@en ,
                             "Proprietà inversa di \"ruolo per\"."@it ;
                
                rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
                
                rdfs:domain :PublicService ;
                
                rdfs:range :RoleInTime .



###  http://dati.gov.it/onto/cpsvapit#isRoleIn

:isRoleIn rdf:type owl:ObjectProperty ;
          
          rdfs:label "is role in"@en ,
                     "è ruolo in"@it ;
          
          rdfs:comment "Inverse property of \"withRole\","@en ,
                       "Proprietà inversa di \"con ruolo\"."@it ,
                       <http://dati.gov.it/onto/cpsvapit> ;
          
          rdfs:domain :Role ;
          
          rdfs:range :RoleInTime .



###  http://dati.gov.it/onto/cpsvapit#isStatusFor

:isStatusFor rdf:type owl:ObjectProperty ;
             
             rdfs:label "is status for"@en ,
                        "è stato per"@it ;
             
             rdfs:comment "Inverse property of \"has status\" used to link the service status with the public service."@en ,
                          "Proprietà inversa di \"ha stato\" utilizzata per connettere uno stato del servizio al servizio pubblico."@it ;
             
             rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
             
             rdfs:range :PublicService ;
             
             rdfs:domain :Status .



###  http://dati.gov.it/onto/cpsvapit#playsARoleInTime

:playsARoleInTime rdf:type owl:ObjectProperty ;
                  
                  rdfs:label "plays a role in time"@en ,
                             "esercita un ruolo nel tempo"@it ;
                  
                  rdfs:seeAlso "http://purl.org/spar/pro#holdsRoleInTime" ;
                  
                  rdfs:comment "This property is used to link an agent to the situation of the role of the agent that changes over time."@en ;
                  
                  rdfs:seeAlso "http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#hasTimeIndexedSetting" ;
                  
                  rdfs:comment "Questa proprietà è utilizzata per connettere un soggetto alla situazione del ruolo del soggetto che varia nel tempo."@it ;
                  
                  rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
                  
                  rdfs:domain :Agent ;
                  
                  rdfs:range :RoleInTime ;
                  
                  owl:inverseOf :isPlayedBy .



###  http://dati.gov.it/onto/cpsvapit#processingTime

:processingTime rdf:type owl:ObjectProperty ;
                
                rdfs:label "processing time"@en ,
                           "tempo di processamento"@it ;
                
                rdfs:comment "This property represents the processing time for a life event. It can be expressed in a quantitative time metric or in a punctual time interval. It connects life event with quantitative value or time interval."@en ,
                             "Questa proprietà rappresenta il tempo di processamento che può essere espresso sia in termini quantitativi, sia in termini puntuali riferibili a un intervallo temporale. La proprietà connette la classe LifeEvent a sia Quantitative Value che Time Interval."@it ;
                
                rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
                
                rdfs:domain :LifeEvent ;
                
                rdfs:range [ rdf:type owl:Class ;
                             owl:unionOf ( :QuantitativeValue
                                           :TimeInterval
                                         )
                           ] .



###  http://dati.gov.it/onto/cpsvapit#produces

:produces rdf:type owl:ObjectProperty ;
          
          rdfs:label "produces"@en ,
                     "produce"@it ;
          
          rdfs:comment "This property is used to connect a public service with the output it produces."@en ,
                       "Questa proprietà è utilizzata per collegare un servizio pubblico con l'output da esso prodotto."@it ;
          
          rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
          
          rdfs:range :Output ;
          
          rdfs:domain :PublicService .



###  http://dati.gov.it/onto/cpsvapit#roleAtTime

:roleAtTime rdf:type owl:ObjectProperty ;
            
            rdfs:label "role at time"@en ,
                       "ruolo nell'intervallo di tempo"@it ;
            
            rdfs:comment "This property is used to link the situation of the role that changes over time with the time interval itself."@en ,
                         "Questa proprietà è utilizzata per collegare la situazione del ruolo che varia nel tempo all'intervallo temporale stesso."@it ;
            
            rdfs:seeAlso "http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#atTime" ;
            
            rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
            
            rdfs:domain :RoleInTime ;
            
            rdfs:range :TimeInterval .



###  http://dati.gov.it/onto/cpsvapit#roleFor

:roleFor rdf:type owl:ObjectProperty ;
         
         rdfs:label "role for"@en ,
                    "ruolo per"@it ;
         
         rdfs:comment "This property is used to link the situation of a role that changes over time to a specific entity. In CPSV-AP_IT the entity is the public service."@en ,
                      "Questa proprietà consente di collegare la situazione di un ruolo che varia nel tempo su uno specifico oggetto. In CPSV-AP_IT l'oggetto è il servizio pubblico."@it ;
         
         rdfs:seeAlso "http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#forEntity" ,
                      "http://purl.org/spar/pro/relatesToEntity" ;
         
         rdfs:isDefinedBy <http://dati.gov.it/onto/cpsvapit> ;
         
         rdfs:range :PublicService ;
         
         rdfs:domain :RoleInTime ;
         
         owl:inverseOf :isRelatedToRiT .



###  http://dati.gov.it/onto/cpsvapit#serviceSubTheme

:serviceSubTheme rdf:type owl:ObjectProperty ;
                 
